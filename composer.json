{
  "name": "workbunny/webman-nacos",
  "type": "library",
  "license": "MIT",
  "description": "Webman plugin workbunny/webman-nacos",
  "authors": [
    {
      "name": "chaz6chez",
      "email": "chaz6chez1993@outlook.com",
      "homepage": "http://chaz6chez.cn"
    }
  ],
  "support": {
    "issues": "https://github.com/workbunny/webman-nacos/issues",
    "source": "https://github.com/workbunny/webman-nacos"
  },
  "require": {
    "php": ">=8.1",
    "ext-json": "*",
    "ext-mbstring": "*",
    "guzzlehttp/promises": "^1.5 | ^2.0",
    "guzzlehttp/guzzle": "^7.0",
    "workerman/http-client": "^1.0 | ^2.0 | ^3.0",
    "monolog/monolog": "^2.8"
  },
  "require-dev": {
    "workerman/webman-framework": "^1.0 | ^2.0",
    "phpunit/phpunit": "^9.0 | ^10.0",
    "friendsofphp/php-cs-fixer": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Workbunny\\WebmanNacos\\": "src"
    },
    "files": [
      "./src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "suggest": {
    "revolt/event-loop": "Better user experience"
  },
  "extra": {
    "branch-alias": {
      "dev-main": "2.x-dev"
    }
  }
}
